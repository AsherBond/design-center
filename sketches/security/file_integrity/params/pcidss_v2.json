{
   "Security::file_integrity" : {
      "hash_algorithm" : "sha256",
      "ifelapsed" : 1440,
      "watch" : [
         "/etc",
         "/root/.ssh",
         "/var/spool/cron",
         "/boot",
         "/bin",
         "/usr/bin",
         "/usr/sbin",
         "/usr/local/bin",
         "/usr/local/sbin",
         "/sbin",
         "/lib",
         "/usr/lib",
         "/usr/lib64"
      ]
   }
}
