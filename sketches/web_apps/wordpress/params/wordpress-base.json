{
   "activated" : true,
   "apache_srv" : {
      "bycontext" : {
         "debian" : "apache2",
         "redhat|centos" : "httpd"
      }
   },
   "htmlroot" : {
      "bycontext" : {
         "debian" : "/var/www",
         "redhat|centos" : "/var/www/html"
      }
   },
   "httpd_process" : {
      "bycontext" : {
         "centos" : "/usr/sbin/httpd",
         "debian" : "/usr/sbin/apache2",
         "redhat" : "^httpd"
      }
   },
   "mysql_process" : {
      "bycontext" : {
         "centos" : "/usr/libexec/mysqld",
         "debian" : "/usr/sbin/mysqld",
         "redhat" : "^mysqld"
      }
   },
   "mysql_srv" : {
      "bycontext" : {
         "debian" : "mysqld",
         "redhat|centos" : "mysqld"
      }
   },
   "packages" : {
      "bycontext" : {
         "debian" : [
            "apache2",
            "php5",
            "php5-mysql",
            "libapache2-mod-php5",
            "mysql-server"
         ],
         "redhat|centos" : [
            "httpd",
            "php",
            "php-mysql",
            "mysql-server"
         ]
      }
   },
   "service_cmd" : {
      "bycontext" : {
         "debian" : "/usr/sbin/service",
         "redhat|centos" : "/sbin/service"
      }
   },
   "wp_dir" : "/var/www/wordpress",
   "wp_params" : {
      "DB_NAME" : "wordpress_db",
      "DB_PASSWORD" : "wordpress_pass",
      "DB_USER" : "wordpress_user"
   }
}
